@using AspNetCoreIdentityApp.MVC.Models.ViewModels
@model List<ClaimViewModel>
@{
    ViewData["Title"] = "Claims";
}


<table class="table table-striped">
    <thead>
        <tr>
            <th>Issuer</th>
            <th>Type</th>
            <th>Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model)
        {
            <tr>
                <td>@claim.Issuer</td>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>


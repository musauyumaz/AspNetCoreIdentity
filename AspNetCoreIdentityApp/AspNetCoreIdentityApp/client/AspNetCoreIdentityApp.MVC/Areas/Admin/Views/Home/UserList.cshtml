@model List<UserViewModel>
@{
    ViewData["Title"] = "UserList";
}

<h1>Üye Listesi</h1>


<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Kullanıcı Adı</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
            </tr>
        }
</table>
